<div class='form-container'>
  <%= form_with model: @pre_registration do |f| %>
    <div class='form-group'>
      <%= f.label :email %>
      <%= f.text_field :email %>
    </div>
    <div class='form-group'>
      <%= f.label :cpf %>
      <%= f.text_field :cpf %>
    </div>    
    <div>
      <%= f.submit class: 'submit-button' %>
    </div>
  <% end %>
</div>